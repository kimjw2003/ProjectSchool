<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ScheduleFragment">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="350dp"
        android:layout_height="120dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/backgrr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <TextView
        android:id="@+id/schedule_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <ImageView
        android:id="@+id/first_grade_Iv"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/first_grade" />

    <ImageView
        android:id="@+id/second_grade_Iv"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="100dp"
        app:layout_constraintEnd_toStartOf="@+id/third_grade_Iv"
        app:layout_constraintStart_toEndOf="@+id/first_grade_Iv"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/second_grade" />

    <ImageView
        android:id="@+id/third_grade_Iv"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/third_grade" />


    <TextView
        android:id="@+id/first_title_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="1학년"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_grade_Iv" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/first_yes_ani"
        android:layout_width="80dp"
        app:lottie_rawRes="@raw/yes"
        app:lottie_autoPlay="true"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_title_Tv" />


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/first_no_ani"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_title_Tv"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/no" />

    <TextView
        android:id="@+id/second_title_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2학년"
        android:textSize="22sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toStartOf="@+id/third_title_Tv"
        app:layout_constraintStart_toEndOf="@+id/first_title_Tv"
        app:layout_constraintTop_toBottomOf="@+id/second_grade_Iv" />



    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/second_yes_ani"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintTop_toBottomOf="@+id/second_title_Tv"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/yes" />


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/second_no_ani"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/second_title_Tv"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/no" />

    <TextView
        android:id="@+id/third_title_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="3학년"
        android:textSize="22sp"
        android:textStyle="bold"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.879"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_grade_Iv" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/third_yes_ani"
        android:layout_width="80dp"
        app:lottie_rawRes="@raw/yes"
        app:lottie_autoPlay="true"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_title_Tv" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/third_no_ani"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_title_Tv"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/no" />

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="396dp"
        android:text="내일의 학사일정"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>